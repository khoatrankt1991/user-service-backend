{
  "rootDir": "..",
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "..",
    "rootDir": "..",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["jest", "node"],
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": [
    "../src/**/*",
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}